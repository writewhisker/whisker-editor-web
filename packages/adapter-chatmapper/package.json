{
  "name": "@writewhisker/adapter-chatmapper",
  "version": "0.1.0",
  "description": "ChatMapper format adapter for Whisker Editor",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@writewhisker/story-models": "workspace:*",
    "@writewhisker/import-parsers": "workspace:*",
    "@writewhisker/export-formats": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "whisker",
    "chatmapper",
    "dialogue",
    "adapter",
    "interactive-fiction"
  ],
  "author": "Whisker Team",
  "license": "MIT"
}
