{
  "name": "@writewhisker/import",
  "version": "0.1.0",
  "description": "Import system for Whisker - JSON and Twine format importers",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "@writewhisker/core-ts": "workspace:*",
    "nanoid": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "typescript": "^5.3.0",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.6.1"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "whisker",
    "import",
    "json",
    "twine",
    "interactive-fiction"
  ],
  "author": "Whisker Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/writewhisker/whisker-editor-web.git",
    "directory": "packages/import"
  }
}
